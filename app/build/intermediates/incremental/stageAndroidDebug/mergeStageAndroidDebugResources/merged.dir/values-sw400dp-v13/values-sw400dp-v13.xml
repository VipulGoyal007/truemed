<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="auto_carousel_padding_horizontal">16dp</dimen>
    <dimen name="auto_carousel_padding_vertical">16dp</dimen>
    <dimen name="chronometer_font_size">20sp</dimen>
    <dimen name="five_cta_icon_margin">8dp</dimen>
    <dimen name="flipper_height">180dp</dimen>
    <dimen name="large_icon">48dp</dimen>
    <dimen name="manual_carousel_arrow">48dp</dimen>
    <dimen name="metadata_font_size">12dp</dimen>
    <dimen name="metadata_margin_x">8dp</dimen>
    <dimen name="metadata_title_margin_vertical">8dp</dimen>
    <dimen name="msg_font_size">14sp</dimen>
    <dimen name="padding_horizontal">16dp</dimen>
    <dimen name="padding_vertical">16dp</dimen>
    <dimen name="product_display_button_height">24dp</dimen>
    <dimen name="rating_big_img">120dp</dimen>
    <dimen name="rating_star">24dp</dimen>
    <dimen name="title_font_size">14sp</dimen>
    <style name="Dialog" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="MetaData" parent="android:Widget.TextView">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="PushMessage" parent="android:Widget.TextView">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="PushMessageMultiLine" parent="android:Widget.TextView">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="PushTitle" parent="android:Widget.TextView">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
</resources>