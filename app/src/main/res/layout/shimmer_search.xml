<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />


    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginHorizontal="@dimen/d16"
        android:background="@color/white"
        android:orientation="vertical">

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="@dimen/d188"
            android:layout_height="@dimen/d10"
            android:layout_marginTop="@dimen/d24"
            android:background="@drawable/shimmer_grey_tube" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/d16"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/d12"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
              />

        </LinearLayout>

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="@dimen/d188"
            android:layout_height="@dimen/d10"
            android:layout_marginTop="@dimen/d32"
            android:background="@drawable/shimmer_grey_tube" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d176"
                android:layout_marginTop="@dimen/d16"
                android:layout_marginEnd="@dimen/d14"
                android:layout_weight="1"
                android:background="@color/white"
                android:theme="@style/Theme.MaterialComponents.Light"
                app:cardCornerRadius="@dimen/d16"
                app:cardElevation="@dimen/d1"
                app:cardPreventCornerOverlap="true"
                app:strokeColor="@color/shimmerGray"
                app:strokeWidth="@dimen/d1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/d16"
                    android:layout_marginVertical="@dimen/d12"
                    android:orientation="vertical">


                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/d120"
                        android:layout_height="@dimen/d89"
                        android:background="@drawable/shimmer_grey_box_12_curve" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/d84"
                        android:layout_height="@dimen/d10"
                        android:layout_marginTop="@dimen/d19"
                        android:background="@drawable/shimmer_grey_tube" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ivTopTube2"
                        android:layout_width="@dimen/d58"
                        android:layout_height="@dimen/d10"
                        android:layout_marginTop="@dimen/d8"
                        android:background="@drawable/shimmer_grey_tube" />

                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d176"
                android:layout_marginTop="@dimen/d16"
                android:layout_weight="1"
                android:background="@color/white"
                android:theme="@style/Theme.MaterialComponents.Light"
                app:cardCornerRadius="@dimen/d16"
                app:cardElevation="@dimen/d1"
                app:cardPreventCornerOverlap="true"
                app:strokeColor="@color/shimmerGray"
                app:strokeWidth="@dimen/d1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/d16"
                    android:layout_marginVertical="@dimen/d12"
                    android:orientation="vertical">


                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/d120"
                        android:layout_height="@dimen/d89"
                        android:background="@drawable/shimmer_grey_box_12_curve" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/d84"
                        android:layout_height="@dimen/d10"
                        android:layout_marginTop="@dimen/d19"
                        android:background="@drawable/shimmer_grey_tube" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/d58"
                        android:layout_height="@dimen/d10"
                        android:layout_marginTop="@dimen/d8"
                        android:background="@drawable/shimmer_grey_tube" />

                </LinearLayout>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="@dimen/d188"
            android:layout_height="@dimen/d10"
            android:layout_marginTop="@dimen/d32"
            android:background="@drawable/shimmer_grey_tube" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/d16"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/d12"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/d36"
                android:layout_marginEnd="@dimen/d10"
                android:layout_weight="1"
                android:background="@drawable/shimmer_grey_box_12_curve" />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                />

        </LinearLayout>

    </LinearLayout>
</layout>